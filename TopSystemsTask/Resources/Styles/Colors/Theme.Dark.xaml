    <Color
        x:Key="InfoCriticalBackgroundColor"
        A="255"
        B="233"
        G="231"
        R="253" />
    <Color
        x:Key="InfoSuccessBackgroundColor"
        A="255"
        B="221"
        G="246"
        R="223" />
    <Color
        x:Key="InfoAttentionBackgroundColor"
        A="127"
        B="246"
        G="246"
        R="246" />
    <Color
        x:Key="InfoCautionBackgroundColor"
        A="255"
        B="206"
        G="244"
        R="255" />
    <!--#endregion-->
    <!--#region Theme text-->
    <Color
        x:Key="TextPrimaryColor"
        A="255"
        B="255"
        G="255"
        R="255" />
    <Color
        x:Key="TextSecondaryColor"
        A="199"
        B="255"
        G="255"
        R="255" />
    <Color
        x:Key="TextTetriaryColor"
        A="138"
        B="255"
        G="255"
        R="255" />
    <Color
        x:Key="TextDisabledColor"
        A="92"
        B="255"
        G="255"
        R="255" />
    <!--#endregion-->
    <!--#endregion-->
</ResourceDictionary>